<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.book.bshop.BookMapper.BookMapper">

<select id="bookList" resultType="kr.book.bshop.BookVO.BookVO">
	select * from tbl_book
</select>

<insert id="bookInsert" parameterType="kr.book.bshop.BookVO.BookVO">
	insert into tbl_book values (#{num},#{title},#{author},#{company},#{isbn},#{count})
</insert>











</mapper>